.base-search{z-index:99;position: fixed;_position: absolute;top:0;left:0;padding:8px 0px 8px 10px;background-color:#0081cc;width:100%;height:32px;text-align:center;}
.base-search-left{position: relative;margin-right:86px;background-position:5px 3px;padding: 4px 20px 4px 32px;background-color: #fff;border-radius:2px;box-shadow: 0 0px 1px #476b9f;}
