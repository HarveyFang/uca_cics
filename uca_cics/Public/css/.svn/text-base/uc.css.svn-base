/*汽车票信息-订单详情*/
.yuding_div .orderinfo_title{background: #0081cc; padding:2px 10px; height:30px; line-height:30px;color:#fff;border-top-left-radius:5px; border-top-right-radius:5px;}
.yuding_div .info_city_box{overflow:hidden;padding:15px 10px; position:relative; clear:both;
border-bottom-left-radius:5px; border-bottom-right-radius:5px; border-top:none;background: #fff;}
.yuding_div .info_city{ height:70px;}
.yuding_div .info_city .grid-start,.yuding_div .info_city .grid-end{overflow:hidden;width:33.3%;float:left;text-align: center;line-height: 35px;color: #333;}
.yuding_div .info_city .grid-jiantou {font-size: 15px;text-align: center;min-width: 40%; position:absolute; left:50%;margin-left:-20%; top:18px;  }
.yuding_div .info_city .grid-jiantou p{ position:relative;display:inline-block; border-bottom:3px solid #0081cc; padding:4px 2px 2px;margin-bottom:13px;color: #0081cc;height:20px;line-height:20px; vertical-align: top;text-align:center;text-overflow: ellipsis;white-space: nowrap;}
.yuding_div .info_city .grid-jiantou p{ position:relative;display:inline-block;padding:3px 10px 3px;margin-bottom:13px;color: #0081cc;height:20px;line-height:20px; vertical-align: top;text-align:center;text-overflow: ellipsis;white-space: nowrap;}
.yuding_div .info_city .grid-jiantou p:after {content:' ';position:absolute;top:50%; margin-top:13px;right:-7px;display:inline-block;width:100%; padding:0 2px;height:12px; background:url(img/chezhan_bg.png) no-repeat right center; background-size:auto 12px; }
.yuding_div .info_city .grid-jiantou  div{color: #999;height:20px;line-height:20px;margin-top:-10px; position:relative;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}

.yuding_div .info_city .grid-jiantou1 {top:2px;min-width: 30%;margin-left:-15%;}
.yuding_div .info_city .grid-jiantou1 p{padding:0px 0px 13px  0;}

.ellipsis{overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.yuding_div .info_city .grid-end{float:right;}
.yuding_div .info_city .grid-start p,.yuding_div .info_city .grid-end p {color: #333; font-size:16px;height:28px;line-height:28px;position:relative;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.yuding_div .info_city .grid-start span,.yuding_div .info_city .grid-end span{color: #999;}

.yuding_small .info_city{ height:40px;}
.yuding_small .info_city .grid-jiantou {margin-left:-20%; top:2px;  }
.yuding_small .info_city .grid-jiantou .topjt{position:relative;display:inline-block; border-bottom:3px solid #0081cc; padding:3px 10px 3px;margin-bottom:13px;color: #0081cc;height:20px;line-height:20px; vertical-align: top;text-align:center;text-overflow: ellipsis;white-space: nowrap;border-bottom:3px solid #0081cc;width:25px;}
.yuding_small .info_city .grid-jiantou .topjt:before {content:' ';position:absolute;top:50%; margin-top:7px;right:0px;display:inline-block;border:#0081cc solid;border-width:3px 0px 0 0;width:16px;height:8px;}
.yuding_small .info_city .grid-jiantou .topjt:before{transition-duration:0;-webkit-transition-duration:0;transform:rotate(45deg);-webkit-transform:rotate(45deg);}



