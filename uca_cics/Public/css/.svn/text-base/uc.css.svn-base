.traicon {width: 20px;height: 20px;line-height:20px; border-radius:3px;text-align: center;font-size: 13px;color: #ffffff;margin-right: 8px; float:left;}
.traicon {position:relative;}
.traicon:after {content:' ';position:absolute;top:50%; margin-top:-6px;right:-5px;display:inline-block;
width:0;height:0;border-top:6px solid transparent;border-bottom: 6px solid transparent;}
.traicon.begin:after{border-left: 5px solid #4db315;}
.traicon.begin {background-color: #4db315;}
.traicon.end:after{border-left: 5px solid #ff8b28;}
.traicon.end {background-color: #ff8b28;}

