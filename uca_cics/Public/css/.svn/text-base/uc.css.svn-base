.base-search-button{width: 58px;height: 32px;line-height: 32px; vertical-align:middle;border-radius: 2px;}
.base-button-common{font-size: 16px;background: #0272b3;text-shadow: 0 0 1px  rgba(0,0,0,.4)}
.base-button-common:active{background: #036daa;}
.base-button-common:hover{background: #036daa;}


.search-box-a .base-search{ position:relative; background:none;padding:0;}
