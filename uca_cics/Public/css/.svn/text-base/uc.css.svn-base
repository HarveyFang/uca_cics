.base-search-input{padding:0;font-weight:400; font-size:16px;-webkit-box-sizing: border-box;width: 100%;height: 23px;line-height:23px;border: none;-webkit-appearance: none;outline-style: none;}
.base-search-close{display: none;width: 21px;height: 21px;background-position:;position: absolute;top: 8px;right: 5px;}
.base-button{display: inline-block;color: #fff;text-align: center;border: none;outline: none;}
input::-moz-placeholder, textarea::-moz-placeholder { color: #b1b4b6;}
